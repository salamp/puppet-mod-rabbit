[host_normal]
<% if @rabbit_config['AdmHostName'] %> hostname <%= @rabbit_config['AdmHostName'] %> <% end %>
<% if @rabbit_config['AdmPort'] %> port <%= @rabbit_config['AdmPort'] %> <% end %>
<% if @rabbit_config['AdmUsername'] %> username <%= @rabbit_config['AdmUsername'] %> <% end %>
<% if @rabbit_config['AdmPass'] %> password <%= @rabbit_config['AdmPass'] %> <% end %>

